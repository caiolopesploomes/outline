# examples/.env.oci
# --- Core ---
URL=https://wiki.example.com
SECRET_KEY=change_me

# --- Database / Redis (adjust as needed) ---
DATABASE_URL=postgres://outline:outline@postgres:5432/outline
REDIS_URL=redis://redis:6379

# --- Auth (adjust to your provider) ---
# GOOGLE_CLIENT_ID=...
# GOOGLE_CLIENT_SECRET=...

# --- S3-compatible (OCI) ---
AWS_ACCESS_KEY_ID=<OCI_ACCESS_KEY_ID>
AWS_SECRET_ACCESS_KEY=<OCI_SECRET_KEY>
AWS_REGION=sa-vinhedo-1
AWS_S3_UPLOAD_BUCKET_URL=https://<NAMESPACE>.compat.objectstorage.sa-vinhedo-1.oraclecloud.com
AWS_S3_UPLOAD_BUCKET_NAME=<bucket_name>
AWS_S3_FORCE_PATH_STYLE=false

# keep to satisfy Outline's validation; patched code ignores it
AWS_S3_ACL=private

# CORS / uploads
FILE_UPLOAD_MAX_SIZE=52428800
